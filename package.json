{
  "name": "okcollege-service-tests",
  "version": "0.0.0",
  "description": "OkCollege Service Testing",
  "engineStrict": true,
  "engines": {
    "node": ">= 6.2"
  },
  "devDependencies": {
    "babel-cli": "~6.18.0",
    "babel-core": "~6.20.0",
    "babel-eslint": "~7.1.1",
    "babel-jest": "~17.0.2",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-plugin-transform-async-to-generator": "~6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "~6.18.0",
    "babel-plugin-transform-flow-strip-types": "~6.18.0",
    "babel-plugin-transform-runtime": "~6.15.0",
    "babel-preset-stage-3": "~6.17.0",
    "eslint": "~3.11.1",
    "eslint-config-airbnb-base": "~10.0.1",
    "eslint-plugin-flowtype": "~2.29.1",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-no-async-without-await": "~1.2.0",
    "flow-bin": "~0.37.0",
    "jest": "^18.0.0"
  },
  "scripts": {
    "clean": "rm -rf coverage dist tmp",
    "lint": "eslint .",
    "postlint": "npm run typecheck",
    "typecheck": "flow check",
    "test": "npm run test-only",
    "test-only": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "build": "babel src/**/*.js -d dist",
    "build:watch": "babel --watch src/**/*.js -d dist"
  },
  "author": "Severin Ibarluzea",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "automock": false,
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "babel-runtime": "~6.20.0",
    "dockerode": "^2.3.1",
    "redis": "^2.6.3",
    "request": "^2.79.0"
  }
}
